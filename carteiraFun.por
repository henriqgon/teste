programa
{
	
	funcao inicio()
	{
		inteiro codFun, idade, ID
		caracter tpsangue

		escreva("Digite o código do Funcuionário: ")
		leia(codFun)

		escreva("Digite a idade do funcionário: ")
		leia(idade)

		escreva("Digite o tipo sanguíneo do funcionário: ")
		leia(tpsangue)

		escreva("Digite a identidade do funcionário: ")
		leia(ID)

		escreva("Carteira de identificação de funcionário é:", "\nCódigo: ", codFun, "\nIdade: ", idade, "\nTipo S: ", tpsangue, "\nID: ", ID)
	}
}
